pipeline {
  agent any
       
     }
    stages {
        stage ("Build") {
           steps {
              sh "npm install"
      }
    }
      stage('install playwright') {
        steps {
          sh 'npm playwright install'
      }
    }
    stage('help') {
      steps {
        sh 'npx playwright test --help'
      }
    }
    stage('test') {
      steps {
        sh '''
          npx playwright test --list
          npx playwright test
        '''
      }
    }
  }

